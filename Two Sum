
lis = [2, 10, 9, 5, 6, 7, 3, 2]

target = 17

flag = 0
for i in range(0, len(lis)):
    for j in range(i, len(lis)):
        if lis[i] + lis[j] == target:
            print("Two sum are: "+ str(lis[i]) + " and " + str(lis[j]))
            flag = 1
            break
    if flag == 1:
        break
    
    
    
    
